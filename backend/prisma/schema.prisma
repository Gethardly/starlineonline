// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Position {
  id          Int      @id @default(autoincrement())
  x           Float
  y           Float
  name        String
  address     String
  contacts    String
  description String
  note        String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
